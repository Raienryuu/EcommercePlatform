{
	"Routes": [
		{
			"DownstreamPathTemplate": "/api/v1/user/login",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host": "localhost",
					"Port": 5156
				}
			],
			"UpstreamPathTemplate": "/api/v1/user/login"
		},
		{
			"DownstreamPathTemplate": "/api/v1/user/register",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host": "localhost",
					"Port": 5156
				}
			],
			"UpstreamPathTemplate": "/api/v1/user/register"
		},
		{
			"DownstreamPathTemplate": "/api/v1/user/{everything}",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host": "localhost",
					"Port": 5156
				}
			],
			"UpstreamPathTemplate": "/api/v1/user/{everything}",
			"UpstreamHttpMethod": [ "GET", "POST" ],
			"AuthenticationOptions": {
				"AuthenticationProviderKeys": [ "Bearer" ]
			},
			"AddHeadersToRequest": {
				"UserId": " Claims[http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name] > value"
			}
		},

		{
			"DownstreamPathTemplate": "/api/v1/product/{everything}",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host": "localhost",
					"Port": 5286
				}
			],
			"UpstreamPathTemplate": "/api/v1/product/{everything}",
			"UpstreamHttpMethod": [ "GET" ]
		}
	],
	"GlobalConfiguration": {
		"BaseUrl": "http://localhost:5044",

	}
}